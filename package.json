{
  "name": "project-name",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "custom-elements-manifest analyze & vite",
    "new": "node ./utils/new.js",
    "create": "node ./utils/create.js",
    "intent": "node ./utils/intent.js",
    "save": "node ./utils/save.js",
