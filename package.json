    "@typescript-eslint/parser": "^5.21.0",
    "axe-core": "^4.4.2",
    "boxen": "^6.2.1",
    "chalk": "^5.0.1",
    "enquirer": "^2.3.6",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-base-typescript": "^1.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
